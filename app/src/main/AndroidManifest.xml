<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cl.jav.superheroes">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name="cl.jav.superheroes.model.SuperApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_background"
        android:label="SuperHeroes"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DesafioSuperHeroesPrueba">
        <activity android:name="cl.jav.superheroes.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>